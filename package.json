{
  "name": "roataway-web",
  "version": "0.1.3",
  "private": true,
  "scripts": {
    "deploy-hash": "export REACT_APP_DEPLOY_HASH=$(git rev-parse --short HEAD)",
    "deploy-date": "export REACT_APP_DEPLOY_DATE=$(git show -s --format=%ci HEAD)",
    "deploy-info": "npm run deploy-hash && npm run deploy-date",
    "prestart": "npm run deploy-info",
    "prebuild": "npm run deploy-info",
    "pretest": "npm run deploy-info",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.4",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.44",
    "@roataway/infrastructure-data": "^1.0.21",
    "@sentry/browser": "^5.12.4",
    "@types/jest": "^24.9.1",
    "@types/leaflet": "^1.5.9",
    "@types/lodash.uniqby": "^4.7.6",
    "@types/node": "^12.12.28",
    "@types/react": "^16.9.22",
    "@types/react-dom": "^16.9.5",
    "@types/react-leaflet": "^2.5.0",
    "@types/sockjs-client": "^1.1.1",
    "csv-parse": "^4.8.6",
    "csv.macro": "^1.0.1",
    "gh-pages": "^2.2.0",
    "husky": "^3.1.0",
    "i18next": "^21.3.2",
    "i18next-browser-languagedetector": "^6.1.2",
    "leaflet": "^1.6.0",
    "lint-staged": "^9.5.0",
    "lodash.uniqby": "^4.7.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hook-form": "^6.8.6",
    "react-i18next": "^11.12.0",
    "react-leaflet": "^2.6.1",
    "react-scripts": "^3.4.4",
    "sass": "^1.43.2",
    "sockjs-client": "^1.4.0",
    "typescript": "^3.8.2",
    "webstomp-client": "^1.2.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
